<!DOCTYPE html>
<html>
  <head>
    <title>Add User</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    
    <h1> List Users</h1>
    
     <table>
         
             <%data.forEach(function(users){%>
                <tr>
                    <td><%= users._id%></td>
                    <td><%= users.name%></td>
                    <td><%= users.email%></td>
                    <td><%= users.ext%></td>
                    <td><%= users.roomno%></td>

               <td>     <img src="/img/<%=users.img%>" width="100" height="100"></td>
                       
                   
                </tr>
            <% }) %>
         </table>
         

  </body>
</html>
